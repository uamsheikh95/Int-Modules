<odoo>
			<record model="ir.ui.view" id="freight_booking_form">
							<field name="name">shardi_logistics_dubai.freight_booking.form</field>
							<field name="model">shardi_logistics_dubai.freight_booking</field>
							<field name="arch" type="xml">
									<form string="Freight Booking Form">

											<header>
													<button string="Create Invoice" type="object" name="invoice_create" class="oe_highlight" attrs="{'invisible': [('state', 'in', ['quotation', 'invoiced','departed'])]}" />
													<button string="Confirm" type="object" name="confirm"
																	confirm="Are you sure you want to Confirm?"
																	class="oe_highlight" attrs="{'invisible': [('state', '!=', 'quotation')]}"/>
													<button string="Generate T.N." type="object" name="generate_tn" />

													<field name="state" widget="statusbar"/>
											</header>

											<sheet>

													<div class="oe_title" style="float:left;width:50%;margin-top:20px">
															<h1>
																	<field name="name" string="Job" attrs="{'invisible': [('state', '=', 'quotation')]}"/>
																	<field name="quote_name" string="Quote" attrs="{'invisible': [('state', '!=', 'quotation')]}"/>
															</h1>
															<div name="options_active">
																	<div>
																			<label for="shipment_type" class="oe_inline" string="Shipment Type :"/>
																			<field name="shipment_type" class="oe_inline oe_input_align" />
																	</div>
															</div>
													</div>
													<div class="oe_button_box" name="button_box">

																<button name="invoice_create" type="object" class="oe_stat_button" icon="fa-dollar" attrs="{'invisible': [('invoice_id', '=', False)]}">
																			Invoice
																			<field name="invoice_id" invisible="1"/>
																</button>
																	<button type="object"  name="bill_create"
																			class="oe_stat_button"
																			icon="fa-pencil-square-o" attrs="{'invisible': [('state', '=', 'quotation')]}">
																			<field name="bill_count" widget="statinfo" string="Bills" />

																			<field name='bill_ids' invisible="1" context="{'default_freight_booking_id': id}"/>
																	</button>
													</div>
													<group>
															<group>
																	<field name="shipper_id" context="{'default_shipper':1}" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"/>
																	<field name="port_of_loading_id" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"
																	 />
																	<field name="port_of_discharge_id" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"
																		/>
																	<field name="port_of_delivery_id" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"
																		/>
																	<field name="customs_doc_ref"
																	 />
																	<field name="carrier_id" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"
																	 />
																	<field name="bill_of_landing"
																	 />
															</group>
															<group>
																	<field name="partner_id" string="Consignee" context="{'default_consignee':1}"
																				 options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"
																	 />
																	<field name="partner_ref"
																	 />

																	<field name="job_date"
																	 />

																	<field name="hawb"
																	 />
																	<field name="mawb"
																	 />
																	<field name="qty" readonly="1"/>
																	<field name="actual_weight" readonly="1"/>
																	<field name="chargeable_weight" readonly="1" string="Total CBM"/>
															</group>
													</group>

													<notebook>
															<page string="Packing List">
																	<field name="packing_list_line_ids" context="{'default_freight_booking': id}" >
																			<tree editable="bottom">
																				<field name="name"/>
																					<field name="waybil"/>
																					<field name="description"/>
																					<field name="qty"/>
																					<field name="departed_qty" string="Departed"/>
																					<field name="departed_vol"/>
																					<field name="remaining_vol" />
																					<field name="package"/>
																					<field name="length"/>
																					<field name="width"/>
																					<field name="height"/>
																					<field name="cbm_type" required="0"/>
																					<field name="formula"/>
																					<field name="volume_weight" string="Volume(CBM)"/>
																					<field name="actual_weight"/>
																					<field name="remarks"/>
																					<field name="tracking_no_ids" widget="many2many_tags"/>
																			</tree>
																	</field>
																	<group class="oe_subtotal_footer oe_right">
																			<div class="oe_subtotal_footer_separator oe_inline">
																					<label for="actual_weight"/>
																			</div>
																			<field name="actual_weight" nolabel="1" class="oe_subtotal_footer_separator" widget="monetary"/>
																	</group>
															</page>
															<page string="Cost Estimation">

																	<field name="cost_estimation_line_ids" context="{'default_freight_booking': id}" >
																			<tree editable="bottom">
																					<field name="product_id" options="{'no_create': True, 'no_quick_create': True, 'no_create_edit':True}"/>
																					<field name="name"/>
																					<field name="qty"/>
																					<field name="rate"/>
																					<field name="price_subtotal"/>
																			</tree>
																	</field>
																	<group class="oe_subtotal_footer oe_right">
																			<div class="oe_subtotal_footer_separator oe_inline">
																					<label for="total"/>
																			</div>
																			<field name="total" nolabel="1" class="oe_subtotal_footer_separator" widget="monetary"/>
																	</group>



															</page>

													</notebook>

											</sheet>
											<div class="oe_chatter">
												<field name="message_follower_ids" widget="mail_followers"/>
												<field name="activity_ids" widget="mail_activity"/>
												<field name="message_ids" widget="mail_thread"/>
										</div>
									</form>
							</field>
				</record>

				<record model="ir.ui.view" id="freight_booking_tree">
						<field name="name">shardi_logistics_dubai.freight_booking.tree</field>
						<field name="model">shardi_logistics_dubai.freight_booking</field>
						<field name="arch" type="xml">
								<tree string="Freight Booking">
										<field name="name" string="Job"/>
										<field name="shipment_type"/>
										<field name="shipper_id"/>
										<field name="partner_id" string="Consignee" />
										<field name="amount"/>
										<field name="amount_paid"/>
										<field name="residual"/>
										<field name="total_bills" string="Job Cost"/>
										<field name="state"/>
								</tree>
						</field>
				</record>
				<record model="ir.ui.view" id="freight_booking_search">
						<field name="name">shardi_logistics_dubai.freight_booking.search</field>
						<field name="model">shardi_logistics_dubai.freight_booking</field>
						<field name="arch" type="xml">
								<search string="Freight Booking">
										<field name="name" string="Job"/>
										<field name="shipment_type"/>
										<field name="partner_ref"/>
										<field name="shipper_id"/>
										<field name="partner_id"/>
										<field name="port_of_loading_id" string="Port of Loading"/>
										<field name="port_of_discharge_id" string="Port of Discharge"/>
										<field name="port_of_delivery_id" string="Port of Delivery"/>

										<group expand="0" string="Group By..." colspan="12" col="10">
											<filter string="Pendig" name="total_under_1000" domain="[('state', '=', 'pending')]"/>
											<filter string="Invoiced" name="total_under_1000" domain="[('state', '=', 'invoiced')]"/>
									</group>
								</search>
						</field>
				 </record>

				<record model="ir.ui.view" id="partner_reference_form_view">
						<field name="name">partner.reference</field>
						<field name="model">res.partner</field>
						<field name="inherit_id" ref="base.view_partner_form"/>
						<field name="arch" type="xml">
								<xpath expr="//field[@name='category_id']" position="after">
										<field name="partner_ref" readonly="1" />
								</xpath>
						</field>
