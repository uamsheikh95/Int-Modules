				<record model="ir.ui.view" id="shipment_form">
							<field name="name">shardi_logistics_dubai.shipment.form</field>
							<field name="model">shardi_logistics_dubai.shipment</field>
							<field name="arch" type="xml">
									<form string="Shipment Form">
												<header>
														<button string="Close Shipment" type="object" name="close" class="oe_highlight"
																		confirm="Are you sure you want to Proceed?" attrs="{'invisible': [('state', '=', 'closed')]}"/>

														<!--This will create the statusbar, thanks to the widget. -->
														<field name="state" widget="statusbar" statusbar_visible="open,closed"/>
												</header>
												<sheet>
														<group>
																<group>
																		<field name="name"  attrs="{'readonly':[('state', '=', 'closed')]}"/>
																		<field name="date"  attrs="{'readonly':[('state', '=', 'closed')]}" />
																</group>
																<group>
																		<field name="company_id"  attrs="{'readonly':[('state', '=', 'closed')]}" />
																		<field name="analytic_account_id"  readonly="1" />
																</group>
														</group>

														<notebook>
																<page string="Freight">
																		<field name="tracking_no_ids" readonly="1">
																				<tree>
																						<field name="name"/>
																						<field name="packing_item_id"/>
																						<field name="partner_id"/>
																						<field name="shipper_id"/>
																						<field name="volume_weight"/>
																						<field name="actual_weight"/>
																				</tree>
																		</field>
																</page>
														</notebook>

												</sheet>

											<div class="oe_chatter">
												<field name="message_follower_ids" widget="mail_followers"/>
												<field name="activity_ids" widget="mail_activity"/>
												<field name="message_ids" widget="mail_thread"/>
										</div>
									</form>
							</field>
				</record>

