<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_move_picking_form_company_branch" model="ir.ui.view">
            <field name="name">stock.move.company.branch</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='location_dest_id']" position="after">
                    <field name="company_branch_id" groups="odoo_multi_branch.group_multi_branches" widget="selection" options="{'no_create': True}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>