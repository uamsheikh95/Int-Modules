<?xml version="1.0" encoding="utf-8"?>
<odoo>
		<data noupdate='1'>
      
      <record id="log_fuel_company_branch_rules" model="ir.rule">
          <field name="name">Fleet Vehicle Log Fuel Company Branch</field>
          <field name="model_id" ref="fleet.model_fleet_vehicle_log_fuel"/>
          <field name="global" eval="True"/>
          <field name="domain_force">['|','|',('company_branch_id','=',False), ('company_branch_id', '=', user.company_branch_id.id),('company_branch_id','in',[branch.id for branch in user.company_branch_ids])]</field>
      </record>

      <record id="log_services_company_branch_rules" model="ir.rule">
          <field name="name">Fleet Vehicle Log Fuel Company Branch</field>
          <field name="model_id" ref="fleet.model_fleet_vehicle_log_services"/>
          <field name="global" eval="True"/>
          <field name="domain_force">['|','|',('company_branch_id','=',False), ('company_branch_id', '=', user.company_branch_id.id),('company_branch_id','in',[branch.id for branch in user.company_branch_ids])]</field>
      </record>

    </data>
</odoo>
