                       <field name = "meter_reader_id" />
                       <field name = "invoice_origin" />
                       <field name = "amount_total_signed" />
                       <field name = "amount_residual_signed" />
                       <field name = "state" invisible="1" />
                     </tree>
                   </field>
                 </page>
               </notebook>
             </xpath>
      </field>
  </record>


  <record id="invoice_filter_search" model="ir.ui.view">
      <field name="name">ACCOUNT MOVE SEARCH VIEW INHERIT</field>
      <field name="model">account.move</field>
      <field name="type">search</field>
      <field name="inherit_id" ref="account.view_account_invoice_filter" />
      <field name="arch" type="xml">
           <xpath expr="//filter[@name='draft']" position="before">
              <filter string="Today" name="today" domain="[('invoice_date', '=', datetime.datetime.now())]"/>
           </xpath>

           <xpath expr="//filter[@name='invoice_date']" position="before">
             <!-- <filter string='Zone' domain="[]" context="{'group_by' : 'zone_id'}"/> -->
             <filter string="Property" name="zone" context="{'group_by': 'property_id'}"/>
             <filter string="Meter Reader" name="zone" context="{'group_by': 'meter_reader_id'}"/>
           </xpath>
      </field>
  </record>



