  <record id="payment_outstanding_inv" model="ir.ui.view">
      <field name="name">account.payment.outstanding.tree</field>
      <field name="model">account.payment</field>
      <field name="inherit_id" ref="account.view_account_payment_form" />
      <field name="type">form</field>
      <field name="arch" type="xml">
             <xpath expr="//sheet" position="inside">
               <notebook>
                 <page string="Outstanding Invoices" attrs="{'invisible': [('partner_id', '=', False)]}">
                   <group>
                     <group>
                       <field name="reconcile_by" widget="radio" />
                     </group>
                     <group>
                     </group>
                   </group>
                   <field name="outstanding_invoice_ids" readonly="1" >
